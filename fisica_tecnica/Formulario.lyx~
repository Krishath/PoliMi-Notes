\begin_layout Subsection
Conduzione
\end_layout

\begin_layout Subsubsection
Unità di Misura
\end_layout

\begin_layout Description
Potenza
\begin_inset space ~
\end_inset

Generata
\begin_inset space ~
\end_inset

su
\begin_inset space ~
\end_inset

Unità
\begin_inset space ~
\end_inset

di
\begin_inset space ~
\end_inset

Volume 
\begin_inset Formula $\sigma=[\frac{W}{m^{3}}]$
\end_inset

;
\end_layout

\begin_layout Subsubsection
Generale
\end_layout

\begin_layout Itemize
\begin_inset Formula $\dot{Q}=-\frac{1}{R_{forma}}\Delta T$
\end_inset

;
\end_layout

\begin_layout Itemize
\begin_inset Formula $\dot{Q}=-kA\frac{dT}{dr}$
\end_inset

;
\end_layout

\begin_layout Itemize
\begin_inset Formula $\dot{Q}=cost$
\end_inset

;
\end_layout

\begin_layout Subsection
Convezione
\end_layout

\begin_layout Itemize
\begin_inset Formula $\dot{Q}_{CONV}=hA(T_{s}-T_{f})$
\end_inset

 con 
\begin_inset Formula $T_{s}=$
\end_inset

 temperatura del solido e 
\begin_inset Formula $T_{f}=$
\end_inset

 temperatura del fluido;
\end_layout

\begin_layout Subsubsection
Raggio Critico di Isolamento
\end_layout

\begin_layout Itemize
\begin_inset Formula $\dot{Q}_{MAX}(\frac{d\dot{Q}}{dr^{2}})=0\,\Rightarrow r_{cr}=\frac{k_{isolante}}{h}$
\end_inset

;
\end_layout

\begin_layout Subsubsection
Convezione Forzata
\end_layout

\begin_layout Itemize
\begin_inset Formula $Nu=$
\end_inset


\begin_inset Formula $\frac{hD}{k}$
\end_inset

, numero di Nusselt;
\end_layout

\begin_layout Itemize
\begin_inset Formula $Re=\frac{\rho wD}{\mu}$
\end_inset

, numero di Reynolds; 
\end_layout

\begin_layout Itemize
\begin_inset Formula $Pr=\frac{c_{p}\mu}{k}$
\end_inset

, numero di Prandtl, indipendente dalla pressione;
\end_layout

\begin_layout Itemize
\begin_inset Formula $Pe=Re\cdot Pr=\frac{wD}{a}$
\end_inset

, numero di Peclet;
\end_layout

\begin_layout Subsubsection
Convezione Naturale
\end_layout

\begin_layout Itemize
\begin_inset Formula $Gr=\frac{\rho^{2}g\beta\Delta TD^{3}}{\mu^{2}}$
\end_inset

, numero di Grashoff;
\end_layout

\begin_layout Itemize
\begin_inset Formula $Ra=Gr\cdot Pr=\frac{g\beta\Delta TD^{3}}{av}$
\end_inset

, numero di Rayleigh;
\end_layout

