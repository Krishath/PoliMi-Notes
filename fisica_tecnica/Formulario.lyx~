\begin_layout Subsubsection
Costanti
\end_layout

\begin_layout Description
Costante
\begin_inset space ~
\end_inset

di
\begin_inset space ~
\end_inset

Stefan-Boltzmann 
\begin_inset Formula $\sigma=5.67\cdot10^{-8\,}[\frac{W}{m^{2}\cdot\text{°\ensuremath{K^{4}}}}]$
\end_inset

;
\end_layout

\begin_layout Description
Costante
\begin_inset space ~
\end_inset

Solare 
\begin_inset Formula $I_{S}=1353\,\frac{W}{m^{2}}$
\end_inset

;
\end_layout

\begin_layout Subsubsection
Generale
\end_layout

\begin_layout Itemize
\begin_inset Formula $\dot{Q}_{IRR}=\varepsilon\sigma A(T_{s})^{4}$
\end_inset

 con 
\begin_inset Formula $\varepsilon=$
\end_inset

 emissività 
\begin_inset Formula $(0\leq\varepsilon\leq1)$
\end_inset

;
\end_layout

\begin_layout Itemize
\begin_inset Formula $E_{n}=\varepsilon\sigma T^{4}$
\end_inset

, potere emissivo di un corpo alla temperatura 
\begin_inset Formula $T$
\end_inset

;
\end_layout

\begin_layout Itemize
\begin_inset Formula $(\lambda\cdot T)_{max\,potenza}=2897,8\,[\mu m\cdot\text{°K]}$
\end_inset

;
\end_layout

\begin_layout Itemize
\begin_inset Formula $\varepsilon(T)=\frac{E(T)}{E_{n}(T)}=\frac{E(T)}{\sigma T^{4}}$
\end_inset

;
\end_layout

\begin_layout Itemize
\begin_inset Formula $R_{IRR}=\varepsilon\sigma(T_{supp}^{2}+T_{amb}^{2})(T_{supp}+T_{amb})=\frac{1}{h_{irr}\cdot A}$
\end_inset


\end_layout

\begin_layout Subsubsection
Il fattore di vista
\end_layout

\begin_layout Standard
Il fattore di vista tra una superficie 
\begin_inset Formula $i$
\end_inset

 e una superficie 
\begin_inset Formula $j$
\end_inset

 si indica 
\begin_inset Formula $F_{i\rightarrow j}$
\end_inset

 e si definisce 
\begin_inset Quotes fld
\end_inset

Frazione della radiazione emessa dalla superficie 
\begin_inset Formula $i$
\end_inset

 che incide direttamente sulla superficie 
\begin_inset Formula $j$
\end_inset


\begin_inset Quotes frd
\end_inset

.
 
\begin_inset Formula $(F\in\mathbb{R};\,0\leq F\leq1)$
\end_inset


\end_layout

\begin_layout Standard
E.g:
\end_layout

\begin_layout Itemize
\begin_inset Formula $F_{i\rightarrow j}=0$
\end_inset

, le superfici 
\begin_inset Formula $i$
\end_inset

 e 
\begin_inset Formula $j$
\end_inset

 non sono in vista tra loro;
\end_layout

\begin_layout Itemize
\begin_inset Formula $F_{i\rightarrow j}=1$
\end_inset

, la superficie 
\begin_inset Formula $j$
\end_inset

 circonda completamente la 
\begin_inset Formula $i$
\end_inset

, per cui tutta la radiazione emessa da 
\begin_inset Formula $i$
\end_inset

 è intercettata da 
\begin_inset Formula $j$
\end_inset

;
\end_layout

\begin_layout Standard
Valgono le seguenti regole:
\end_layout

\begin_layout Itemize
\begin_inset Formula $F_{i\rightarrow j}=F_{j\rightarrow i}$
\end_inset

, se e solo se 
\begin_inset Formula $A_{i}=A_{j}$
\end_inset

;
\end_layout

\begin_layout Itemize
\begin_inset Formula $F_{i\rightarrow j}\neq F_{j\rightarrow i},$
\end_inset

 se e solo se 
\begin_inset Formula $A_{i}\neq A_{j}$
\end_inset

;
\end_layout

\begin_layout Itemize
\begin_inset Formula $A_{i}F_{i\rightarrow j}=A_{j}F_{j\rightarrow i}$
\end_inset

;
\end_layout

\begin_layout Itemize
\begin_inset Formula $\sum_{j=1}^{n}F_{i\rightarrow j}=1$
\end_inset

;
\end_layout

\begin_layout Itemize
\begin_inset Formula $F_{A_{12}\rightarrow_{3}}\cdot(A_{1}+A_{2})=F_{A_{1}\rightarrow A_{3}}\cdot A_{1}+F_{A_{2}\rightarrow A_{3}}\cdot A_{2}$
\end_inset

;
\end_layout

\begin_layout Subsubsection
Coefficienti di assorbimento, riflessione e trasmissione
\end_layout

\begin_layout Description
Assorbimento 
\begin_inset Formula $\alpha=\frac{I_{ass}}{I}$
\end_inset

;
\end_layout

\begin_layout Description
Riflessione 
\begin_inset Formula $\rho=\frac{I_{rifl}}{I}$
\end_inset

;
\end_layout

\begin_layout Description
Trasmissione 
\begin_inset Formula $\tau=\frac{I_{tr}}{I}$
\end_inset

;
\end_layout

\begin_layout Standard
Sussistono inoltre le seguenti relazioni:
\end_layout

\begin_layout Itemize
\begin_inset Formula $\alpha+\rho+\tau=1$
\end_inset

, per superfici trasparenti;
\end_layout

\begin_layout Itemize
\begin_inset Formula $\alpha+\rho=1$
\end_inset

, per superfici opache;
\end_layout

\begin_layout Itemize
\begin_inset Formula $\alpha=\varepsilon$
\end_inset

 quando la differenza di temperatura tra due corpi 
\begin_inset Formula $\Delta T<100\text{°K}$
\end_inset

;
\end_layout

\begin_layout Subsubsection
Convenzioni per lo scambio termico tra superfici
\end_layout

\begin_layout Description
\begin_inset Formula $q_{1\rightarrow2}$
\end_inset

 Potenza termica per unità di superficie emessa dalla superficie 1 che incide
 sulla superficie 2;
\end_layout

\begin_layout Description
\begin_inset Formula $q_{1-2}$
\end_inset

 Potenza termica per unità di superficie emessa dalla superficie 1 che viene
 assorbita dalla superficie 2;
\end_layout

\begin_layout Description
\begin_inset Formula $q_{1,2}$
\end_inset

 Potenza termica netta per unità di superficie scambiata tra la superficie
 1 e la superficie 2;
\end_layout

\begin_layout Standard
Inoltre valgono anche:
\end_layout

\begin_layout Itemize
\begin_inset Formula $q_{1-2}=\alpha\cdot q_{1\rightarrow2}$
\end_inset

;
\end_layout

\begin_layout Itemize
\begin_inset Formula $q_{1,2}=q_{1-2}-q_{2-1}=-q_{2,1}$
\end_inset

;
\end_layout

\begin_layout Subsubsection
Scambio termico tra superfici
\end_layout

\begin_layout Standard
Genericamente vale:
\end_layout

\begin_layout Itemize
\begin_inset Formula $\dot{Q}_{1,2}=A_{1}F_{12}\sigma_{0}(T_{1}^{4}-T_{2}^{4})$
\end_inset

;
\end_layout

\begin_layout Standard
In caso di superfici piane parallele indefinite si ha:
\end_layout

\begin_layout Standard
Per una superficie grigia opaca:
\end_layout

\begin_layout Itemize
\begin_inset Formula $\begin{cases}
J_{i}=E_{i}+\rho I_{i}=\epsilon_{i}E_{in}+(1-\epsilon_{i})I_{i}; & \epsilon=\alpha,\,\,\,\alpha+\rho=1;\\
\dot{Q}_{i}=A_{i}(J_{i}-I_{i});
\end{cases}$
\end_inset


\end_layout

\begin_layout Standard
Per superfici grigie formanti una cavità:
\end_layout

\begin_layout Itemize
\begin_inset Formula $\dot{Q}_{1,2}=\frac{\sigma_{0}(T_{1}^{4}-T_{2}^{4})}{\frac{1-\epsilon_{1}}{\epsilon_{1}A_{1}}+\frac{1}{A_{1}F_{12}}+\frac{1-\epsilon_{2}}{\epsilon_{2}A_{2}}}$
\end_inset

;
\end_layout

\begin_layout Standard
\begin_inset Newpage clearpage
\end_inset


\end_layout

