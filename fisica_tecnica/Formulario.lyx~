	height 25pheight%
	keepAspectRatio

\end_inset


\end_layout

\begin_layout Itemize
\begin_inset Formula $\eta_{D}=1-\frac{c_{v}T_{1}(\frac{T_{4}}{T_{1}}-1)}{c_{p}T_{2}(\frac{T_{3}}{T_{2}}-1)}$
\end_inset

;
\end_layout

\begin_layout Itemize
\begin_inset Formula $\eta=1-\frac{T_{4}-T_{1}}{n(T_{3}-T_{2})}$
\end_inset

;
\end_layout

\begin_layout Itemize
\begin_inset Formula $\eta=1-\frac{1}{r^{n-1}}\cdot[\frac{z^{n}-1}{n(z-1)}]$
\end_inset

;
\end_layout

\begin_layout Itemize
\begin_inset Formula $r=\frac{V_{1}}{V_{2}}$
\end_inset

, rapporto di compressione volumetrico;
\end_layout

\begin_layout Itemize
\begin_inset Formula $z=\frac{V_{3}}{V_{2}}$
\end_inset

, rapporto di combustione;
\end_layout

