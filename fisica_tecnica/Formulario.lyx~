\begin_layout Subsection
Generale e Proprietà Gas
\end_layout

\begin_layout Standard
Da tenere sempre presente che
\begin_inset Formula 
\[
P\bar{v}=R^{*}T
\]

\end_inset

riscrivibile come
\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
PV=nRT
\]

\end_inset


\end_layout

\begin_layout Standard
Inoltre, il 
\begin_inset Formula $I$
\end_inset

 principio della termodinamica impone che
\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
\Delta U=\overleftarrow{Q}-\overrightarrow{L}
\]

\end_inset


\end_layout

\begin_layout Itemize
\begin_inset Formula $R=8314,4\frac{J}{\text{°K}\cdot kmol}$
\end_inset

;
\end_layout

\begin_layout Itemize
\begin_inset Formula $n=\frac{m}{M}$
\end_inset

 dove 
\begin_inset Formula $n$
\end_inset

 = numero di moli;
\end_layout

\begin_layout Itemize
\begin_inset Formula $c_{p}-c_{v}=R^{*}$
\end_inset

, la relazione di Mayer;
\end_layout

\begin_layout Itemize
\begin_inset Formula $\dot{V}=\dot{m}\bar{v}$
\end_inset

 con 
\begin_inset Formula $\dot{V}=$
\end_inset

 portata volumetrica;
\end_layout

\begin_layout Subsection
Calore, Energia Interna, Entalpia ed Entropia
\end_layout

\begin_layout Subsubsection
Calore
\end_layout

\begin_layout Itemize
\begin_inset Formula $Q=mc_{x}\varDelta T$
\end_inset

 con 
\begin_inset Formula $c_{x}=cost$
\end_inset

;
\end_layout

\begin_layout Itemize
\begin_inset Formula $Q=mc_{v}\varDelta T$
\end_inset

 con 
\begin_inset Formula $V=cost$
\end_inset

;
\end_layout

\begin_layout Itemize
\begin_inset Formula $Q=mc_{p}\varDelta T$
\end_inset

 con 
\begin_inset Formula $P=cost$
\end_inset

;
\end_layout

\begin_layout Itemize
\begin_inset Formula $Q=T\Delta S$
\end_inset

 con 
\begin_inset Formula $T=cost$
\end_inset

;
\end_layout

\begin_layout Itemize
\begin_inset Formula $\dot{Q}=\dot{m}c_{materiale}\Delta T$
\end_inset

;
\end_layout

\begin_layout Subsubsection
Energia Interna
\end_layout

\begin_layout Itemize
\begin_inset Formula $d\bar{u}=c_{v}dT$
\end_inset

, se gas perfetto;
\end_layout

\begin_layout Itemize
\begin_inset Formula $d\bar{u}=Tds-Pd\bar{v}$
\end_inset

;
\end_layout

\begin_layout Subsubsection
Entalpia
\end_layout

\begin_layout Itemize
\begin_inset Formula $H=U+PV$
\end_inset

;
\end_layout

\begin_layout Itemize
\begin_inset Formula $d\bar{h}=c_{p}dT$
\end_inset

, se gas perfetto;
\end_layout

\begin_layout Itemize
\begin_inset Formula $d\bar{h}=Tds+\bar{v}dP$
\end_inset

;
\end_layout

\begin_layout Itemize
\begin_inset Formula $d\bar{h}=\delta q+\bar{v}dP$
\end_inset

;
\end_layout

\begin_layout Subsubsection
Entropia
\end_layout

\begin_layout Itemize
\begin_inset Formula $ds=\frac{\delta q}{T}$
\end_inset

, nel caso di un processo reversibile;
\end_layout

\begin_layout Itemize
\begin_inset Formula $\varDelta S=\varDelta S_{\overleftarrow{Q}}+\varDelta S_{irr}$
\end_inset

;
\end_layout

\begin_layout Itemize
\begin_inset Formula $ds=c_{v}\frac{dT}{T}+R^{*}\frac{d\bar{v}}{\bar{v}}$
\end_inset


\begin_inset Formula $\Rightarrow\Delta S=c_{v}ln(\frac{T_{2}}{T_{1}})+R^{*}ln(\frac{V_{2}}{V_{1}});$
\end_inset

 
\end_layout

\begin_layout Itemize
\begin_inset Formula $ds=c_{p}\frac{dT}{T}-R^{*}\frac{dP}{P}\Rightarrow\Delta S=c_{p}ln(\frac{T_{2}}{T_{1}})-R^{*}ln(\frac{P_{2}}{P_{1}});$
\end_inset


\end_layout

\begin_layout Itemize
\begin_inset Formula $ds=c_{p}\frac{d\bar{v}}{\bar{v}}+c_{v}\frac{dP}{P}\Rightarrow\Delta S=c_{p}ln(\frac{V_{2}}{V_{1}})+c_{v}ln(\frac{P_{2}}{P_{1}});$
\end_inset


\end_layout

\begin_layout Itemize
\begin_inset Formula $\Delta S\geq0$
\end_inset

 se il sistema è isolato;
\end_layout

\begin_layout Subsubsection
Liquidi
\end_layout

\begin_layout Itemize
\begin_inset Formula $c_{H_{2}0}=4186\frac{J}{kg\cdot\text{°K}};$
\end_inset


\end_layout

\begin_layout Itemize
\begin_inset Formula $c=c_{v}=c_{p};$
\end_inset


\end_layout

\begin_layout Itemize
\begin_inset Formula $du=\delta q=cdT;$
\end_inset


\end_layout

\begin_layout Itemize
\begin_inset Formula $ds=c\frac{dT}{T}$
\end_inset

;
\end_layout

\begin_layout Itemize
\begin_inset Formula $v=cost$
\end_inset

;
\end_layout

\begin_layout Itemize
\begin_inset Formula $\Delta h=c\Delta T+v\Delta P;$
\end_inset


\end_layout

\begin_layout Subsection
Trasformazioni Politropiche
\end_layout

\begin_layout Itemize
\begin_inset Formula $P\bar{v}^{n}=cost$
\end_inset

;
\end_layout

\begin_layout Itemize
\begin_inset Formula $T\bar{v}^{n-1}=cost$
\end_inset

;
\end_layout

\begin_layout Itemize
\begin_inset Formula $PT^{\frac{n}{1-n}}=cost$
\end_inset

;
\end_layout

\begin_layout Subsection
Rendimenti
\end_layout

\begin_layout Itemize
\begin_inset Formula $\eta_{confronto}=\frac{\eta_{serb-reale}}{\eta_{serb-ideale}};$
\end_inset


\end_layout

\begin_layout Itemize
\begin_inset Formula $\eta_{II_{P}}=\frac{\eta_{reale}}{\eta_{ideale}}=\frac{L_{reale}}{L_{ideale}}$
\end_inset

;
\end_layout

\begin_layout Subsection
Sistemi Bifase
\end_layout

\begin_layout Subsubsection
Generale
\end_layout

\begin_layout Itemize
Le transizioni di fase avvengono con 
\begin_inset Formula $P=costante$
\end_inset

 e 
\begin_inset Formula $T=costante$
\end_inset

;
\end_layout

\begin_layout Itemize
\begin_inset Formula $\begin{cases}
\begin{cases}
x=\ensuremath{\frac{v-v_{l}}{v_{v}-v_{l}};} & con\,x=titolo,\,v=proprietà\,estensiva\\
v_{lv}=v_{v}-v_{l}
\end{cases}\\
\mbox{\ensuremath{v=v_{l}+xv_{lv};}}\\
\mbox{\ensuremath{h=h_{l}+xh_{lv};}}\\
\mbox{\ensuremath{s=s_{l}+xs_{lv};}}\\
\mbox{\ensuremath{u=u_{l}+xu_{lv};}}
\end{cases}\mbox{}$
\end_inset


\end_layout

\begin_layout Itemize
\begin_inset Formula $dh=d\overleftarrow{q};$
\end_inset


\end_layout

\begin_layout Subsubsection
Liquidi sottoraffreddati
\end_layout

\begin_layout Itemize
\begin_inset Formula $h\cong h_{l@T}$
\end_inset

;
\end_layout

\begin_layout Itemize
\begin_inset Formula $s\cong s_{l@T};$
\end_inset


\end_layout

\begin_layout Subsection
Sistemi Aperti
\end_layout

\begin_layout Subsubsection
Generale
\end_layout

\begin_layout Itemize
\begin_inset Formula $\dot{m}=\rho w\varOmega$
\end_inset

 con 
\begin_inset Formula $\varOmega=sezione$
\end_inset

;
\end_layout

\begin_layout Itemize
\begin_inset Formula $\frac{dE}{dt}=\overleftarrow{\dot{m}}[(\overleftarrow{h}-\overrightarrow{h})+g(\overleftarrow{z}-\overrightarrow{z})+\frac{\overleftarrow{w}^{2}-\overrightarrow{w}^{2}}{2}]+\overleftarrow{\dot{Q}}-\overrightarrow{L}$
\end_inset

 con 
\begin_inset Formula $z=altezza$
\end_inset

 e 
\begin_inset Formula $g=accelerazione\,gravitazionale$
\end_inset

;
\end_layout

\begin_layout Itemize
\begin_inset Formula $\frac{dS}{dt}=\overleftarrow{\dot{m}}(\overleftarrow{s}-\overrightarrow{s})+\dot{S_{\overleftarrow{Q}}}+\overrightarrow{\dot{S_{irr}}}$
\end_inset

;
\end_layout

\begin_layout Itemize
\begin_inset Formula $\frac{dm}{dt}=\overleftarrow{m}-\overrightarrow{m};$
\end_inset


\end_layout

\begin_layout Subsubsection
Macchina Aperta
\end_layout

\begin_layout Standard
Dispositivo adiabatico atto a scambiare lavoro per il quale si ipotizzano
 trascurabili le variazioni di energia potenziale e cinetica tra le sezioni
 di ingresso e di uscita.
\end_layout

\begin_layout Itemize
\begin_inset Formula $\overleftarrow{\dot{m}}(\overleftarrow{h}-\overrightarrow{h})-\overrightarrow{L}=0$
\end_inset

;
\end_layout

\begin_layout Itemize
\begin_inset Formula $\overleftarrow{\dot{m}}(\overleftarrow{s}-\overrightarrow{s})+\dot{S}_{irr}=0$
\end_inset

;
\end_layout

\begin_deeper
\begin_layout Subsubsection
Turbina
\end_layout

\begin_layout Itemize
\begin_inset Formula $\eta_{isT}=\frac{\overrightarrow{\dot{L}}_{reale}}{\overrightarrow{\dot{L}}_{ideale}}=\frac{(h_{1}-h_{2^{'}})}{(h_{1}-h_{2})}$
\end_inset

 con 
\begin_inset Formula $\eta_{is}=$
\end_inset

 rendimento isoentropico;
\end_layout

\begin_layout Subsubsection
Compressore / Pompa di Calore
\end_layout

\begin_layout Itemize
\begin_inset Formula $\eta_{isC}=\frac{\overrightarrow{\dot{L}}_{ideale}}{\overrightarrow{\dot{L}}_{reale}}=\frac{(h_{1}-h_{2})}{(h_{1}-h_{2^{'}})}$
\end_inset

;
\end_layout

\end_deeper
\begin_layout Subsubsection
Scambiatore di Calore
\end_layout

\begin_layout Standard
Gli scambiatori sono sistemi aperti stazionari che operano senza scambio
 di lavoro per i quali si ipotizzano trascurabili le variazioni di energia
 potenziale e cinetica tra le sezioni di ingresso e di uscita.
\end_layout

\begin_layout Itemize
\begin_inset Formula $\overleftarrow{\dot{m}}(\overleftarrow{h}-\overrightarrow{h})+\overleftarrow{\dot{Q}}=0$
\end_inset

;
\end_layout

\begin_layout Itemize
\begin_inset Formula $\overleftarrow{\dot{m}}(\overleftarrow{s}-\overrightarrow{s})+\dot{S}_{\overleftarrow{Q}}+\dot{S}_{irr}=0$
\end_inset

;
\end_layout

\begin_layout Itemize
\begin_inset Formula $\overleftarrow{\dot{Q}}=\overrightarrow{\dot{Q}}$
\end_inset


\end_layout

\begin_layout Subsubsection
Diffusore (
\begin_inset Formula $w\downarrow)$
\end_inset

 e Ugello (
\begin_inset Formula $w\uparrow$
\end_inset

)
\end_layout

\begin_layout Standard
I diffusori e gli ugelli sono sistemi aperti stazionari che operano senza
 scambio di lavoro né calore per i quali si ipotizzano trascurabili le variazion
i di energia potenziale tra le sezioni di ingresso e di uscita.
\end_layout

\begin_layout Itemize
\begin_inset Formula $[(\overleftarrow{h}-\overrightarrow{h})+\frac{\overleftarrow{w}^{2}-\overrightarrow{w}^{2}}{2}]=0$
\end_inset

;
\end_layout

\begin_layout Itemize
\begin_inset Formula $\overleftarrow{\dot{m}}(\overleftarrow{s}-\overrightarrow{s})+\dot{S}_{irr}=0$
\end_inset

;
\end_layout

\begin_layout Subsubsection
Valvola di Laminazione
\end_layout

\begin_layout Standard
Le valvole di laminazione sono sistemi aperti stazionari che operano senza
 scambio di lavoro nè calore per i quali si ipotizzano trascurabili le variazion
i di energia potenziale e cinetica tra le sezioni di ingresso e di uscita.
\end_layout

\begin_layout Itemize
\begin_inset Formula $(\overleftarrow{h}-\overrightarrow{h})=0$
\end_inset

;
\end_layout

\begin_layout Itemize
\begin_inset Formula $\overleftarrow{\dot{m}}(\overleftarrow{s}-\overrightarrow{s})+\dot{S}_{irr}=0$
\end_inset

;
\end_layout

\begin_layout Subsection
Macchine Motrici a gas (
\begin_inset Formula $\overrightarrow{L}$
\end_inset

)
\end_layout

\begin_layout Standard
Converte energia termica in lavoro.
 
\end_layout

\begin_layout Subsubsection
Generale
\end_layout

\begin_layout Itemize
\begin_inset Formula $\eta_{reale}=1-\frac{T_{F}}{T_{C}}-\frac{T_{F}\cdot S_{irr}}{Q_{C}}$
\end_inset


\end_layout

\begin_layout Itemize
\begin_inset Formula $\eta_{rev}=1-\frac{T_{F}}{T_{C}}$
\end_inset

;
\end_layout

\begin_layout Itemize
\begin_inset Formula $\Delta S_{irr}=-\frac{\overleftarrow{Q}}{T_{C/F}}+\frac{\overrightarrow{Q}}{T_{C/F}}$
\end_inset

 dove 
\begin_inset Formula $Q$
\end_inset

 è riferito alla macchina e 
\begin_inset Formula $T_{C/F}$
\end_inset

 è la temperatura associata al 
\begin_inset Formula $Q$
\end_inset

 corrispondente;
\end_layout

\begin_layout Subsection
Macchine Operatrici a gas (
\begin_inset Formula $\overleftarrow{L}$
\end_inset

)
\end_layout

\begin_layout Standard
Trasferisce energia termica da uno o più serbatoi di calore a temperatura
 inferiore a uno o più serbatoi di calore a temperatura superiore.
\end_layout

\begin_layout Standard
\begin_inset Graphics
	filename Formulario/MacchinaOperatrice.png
	height 25pheight%

\end_inset


\end_layout

\begin_layout Itemize
\begin_inset Formula $\Delta S_{irr}=-\frac{\overleftarrow{Q}}{T_{C/F}}+\frac{\overrightarrow{Q}}{T_{C/F}}$
\end_inset

 dove 
\begin_inset Formula $Q$
\end_inset

 è riferito alla macchina e 
\begin_inset Formula $T_{C/F}$
\end_inset

 è la temperatura associata al 
\begin_inset Formula $Q$
\end_inset

 corrispondente;
\end_layout

\begin_layout Subsubsection
Macchine frigorifere
\end_layout

\begin_layout Itemize
\begin_inset Formula $L_{rev}=Q_{F}(\frac{T_{C}}{T_{F}}-1)=\frac{Q_{F}}{\varepsilon}$
\end_inset

;
\end_layout

\begin_layout Itemize
\begin_inset Formula $L_{reale}=L_{rev}+T_{C}S_{irr}$
\end_inset

;
\end_layout

\begin_layout Itemize
\begin_inset Formula $\varepsilon_{f}=\frac{Q_{F}}{L}=\frac{T_{F}}{T_{C}-T_{F}+\frac{T_{C}T_{F}S_{irr}}{\overleftarrow{Q}}}$
\end_inset

 ;
\end_layout

\begin_layout Itemize
\begin_inset Formula $\varepsilon_{f_{rev}}=\frac{T_{F}}{T_{C}-T_{F}}$
\end_inset

;
\end_layout

\begin_layout Subsubsection
Macchine calorifere (pompe di calore)
\end_layout

\begin_layout Itemize
\begin_inset Formula $L_{rev}=Q_{C}(1-\frac{T_{F}}{T_{C}})=\frac{Q_{C}}{\varepsilon}$
\end_inset

;
\end_layout

\begin_layout Itemize
\begin_inset Formula $L_{reale}=L_{rev}+T_{F}S_{irr}$
\end_inset

;
\end_layout

\begin_layout Itemize
\begin_inset Formula $\varepsilon_{pdc}=\frac{Q_{C}}{L}=\frac{\overrightarrow{Q}+L}{L}=\varepsilon_{f}+1=\frac{T_{C}}{T_{C}-T_{F}+\frac{T_{C}T_{F}S_{irr}}{\overleftarrow{Q}}}$
\end_inset

;
\end_layout

\begin_layout Itemize
\begin_inset Formula $\varepsilon_{pdc_{rev}}=\frac{T_{C}}{T_{C}-T_{F}}$
\end_inset

;
\end_layout

\begin_layout Section
Cicli
\end_layout

\begin_layout Subsubsection
Generale
\end_layout

\begin_layout Itemize
\begin_inset Formula $\eta=\frac{L_{netto}}{\overleftarrow{Q}};$
\end_inset


\end_layout

\begin_layout Itemize
\begin_inset Formula $n=\frac{c_{p}}{c_{v}};$
\end_inset


\end_layout

\begin_layout Subsection
Cicli simmetrici
\end_layout

\begin_layout Standard
Per i cicli simmetrici valgono le seguenti proprietà:
\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
\bar{v}_{1}\bar{v}_{3}=\bar{v}_{2}\bar{v}_{4}
\]

\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
P_{1}P_{3}=P_{2}P_{4}
\]

\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
T_{1}T_{3}=T_{2}T_{4}
\]

\end_inset


\end_layout

\begin_layout Subsection
Cicli a Gas
