\begin_layout Itemize
\begin_inset Formula $\overleftarrow{Q}=u_{3}-u_{2}=c_{v}(T_{3}-T_{2})$
\end_inset

;
\end_layout

\begin_layout Itemize
\begin_inset Formula $\overrightarrow{Q}=u_{4}-u_{1}=c_{v}(T_{4}-T_{1})$
\end_inset

;
\end_layout

\begin_layout Itemize
\begin_inset Formula $l=c_{v}(T_{3}-T_{4})-c_{v}(T_{2}-T_{1})=c_{v}T_{3}(1-\frac{1}{r_{vol}^{n-1}})-c_{v}T_{1}(r_{vol}^{n-1}-1)=c_{v}T_{3}(1-\frac{T_{4}}{T_{3}})-c_{v}T_{1}(\frac{T_{2}}{T_{1}}-1)$
\end_inset

;
\end_layout

\begin_layout Itemize
