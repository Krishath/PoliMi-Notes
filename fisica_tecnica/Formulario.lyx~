\begin_layout Description
Flusso
\begin_inset space ~
\end_inset

Termico
\begin_inset space ~
\end_inset

Areico 
\begin_inset Formula $\Phi=\dot{q}=\frac{\dot{Q}}{A}$
\end_inset

;
\end_layout

\begin_layout Description
Conducibilità
\begin_inset space ~
\end_inset

Termica 
\begin_inset Formula $k=[\frac{W}{m\cdot\text{°K}}],$
\end_inset

 indipendente dalla pressione;
\end_layout

\begin_layout Description
Coefficiente
\begin_inset space ~
\end_inset

Scambio
\begin_inset space ~
\end_inset

Termico
\begin_inset space ~
\end_inset

Convettivo 
\begin_inset Formula $h=[\frac{W}{m^{2}\cdot\text{°K}}]$
\end_inset

;
\end_layout

\begin_layout Description
Vettore
\begin_inset space ~
\end_inset

Flusso
\begin_inset space ~
\end_inset

di
\begin_inset space ~
\end_inset

Calore 
\begin_inset Formula $\vec{q}=[\frac{W}{m^{2}}]$
\end_inset

;
\end_layout

\begin_layout Description
Viscosità 
\begin_inset Formula $\mu=[Pl]$
\end_inset

, indipendente dalla pressione;
\end_layout

\begin_layout Description
Diametro
\begin_inset space ~
\end_inset

o
\begin_inset space ~
\end_inset

Lunghezza
\begin_inset space ~
\end_inset

Caratteristica 
\begin_inset Formula $D=[m]$
\end_inset

;
\end_layout

\begin_layout Description
Resistenza 
\begin_inset Formula $R=\frac{\text{°}K\cdot m^{2}}{W}$
\end_inset

 oppure 
\begin_inset Formula $R=\frac{\text{°K}}{W}$
\end_inset

;
